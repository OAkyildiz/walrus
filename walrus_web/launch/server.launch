<launch>
  <node name="walrus_web_server" pkg="walrus_web" type="webserver.py">
    <param name="port" value="9000" />
  </node>
  <node name="webrtc_ros_server" pkg="webrtc_ros" type="webrtc_ros_server_node">
    <param name="port" value="9001" />
  </node>
  <node name="ros_package_web_server" pkg="ros_package_web_server" type="server.py">
    <param name="port" value="9002" />
  </node>
  <include file="$(find rosbridge_server)/launch/rosbridge_websocket.launch">
    <arg name="port" value="9003" />
  </include>

  <node name="tf2_web_republisher" pkg="tf2_web_republisher" type="tf2_web_republisher" />

  <node pkg="turtlebot_teleop" type="turtlebot_teleop_joy" name="turtlebot_teleop_joystick">
    <param name="scale_angular" value="1.5"/>
    <param name="scale_linear" value="0.5"/>
    <remap from="turtlebot_teleop_joystick/cmd_vel" to="cmd_vel_mux/input/teleop"/>
  </node>
</launch>
